{
    "name": "@muhimron90/muhi-ui",
    "version": "1.0.6",
    "description": "Muhi-UI React Components",
    "main": "dist/index.js",
    "scripts": {
        "start": "webpack-dev-server --config ./config/webpack/webpack.config.js --mode=development ",
        "dist": " http-server dist",
        "watch": "webpack --progress --config ./config/webpack/webpack.config.js --watch --mode=development",
        "build": "webpack --config ./config/webpack/webpack.config.js --mode=production",
        "sg:dev": "npx styleguidist server --config ./styleguide.config.js",
        "sg:build": "npx styleguidist build ",
        "prettier:all": "prettier --write \"{,!(node_modules)/**/}*.js\"",
        "prettier:src": "prettier --write src/**/*",
        "eslint": "eslint \"src/**/*.{js,jsx,css}\" ",
        "lint": "eslint ./ --ignore-path .gitignore ",
        "lint:fix": "eslint  src/**/*.{js,jsx,css} --fix",
        "test": "echo \"No test specified\"",
        "pub:npm": "np --no-2fa",
        "pretty": "pretty-quick --staged",
        "lints": "lint-staged && npm run pretty"
    },
    "keywords": [
        "muhi-ui",
        "components-library"
    ],
    "author": "Muhamad Imron",
    "license": "MIT",
    "resolutions": {
        "styled-components": "^5"
    },
    "peerDependencies": {
        "react": ">= 16",
        "react-dom": ">= 16",
        "styled-components": ">= 3"
    },
    "dependencies": {
        "core-js": "^3.6.5",
        "prop-types": "^15.7.2",
        "react": "^16.13.1",
        "react-dom": "^16.13.1"
    },
    "devDependencies": {
        "@babel/core": "^7.11.5",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
        "@babel/plugin-syntax-jsx": "^7.10.4",
        "@babel/preset-env": "^7.11.5",
        "@babel/preset-react": "^7.10.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.1.0",
        "babel-plugin-styled-components": "^1.11.1",
        "clean-webpack-plugin": "^3.0.0",
        "css-loader": "^4.2.2",
        "eslint": "^7.8.1",
        "eslint-config-airbnb": "^18.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-plugin-flowtype": "^5.2.0",
        "eslint-plugin-import": "^2.22.0",
        "eslint-plugin-jsx-a11y": "^6.3.1",
        "eslint-plugin-prettier": "^3.1.4",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.0",
        "file-loader": "^6.1.0",
        "html-webpack-plugin": "^4.4.1",
        "http-server": "^0.12.3",
        "husky": "^4.2.5",
        "lint-staged": "^10.3.0",
        "mini-css-extract-plugin": "^0.11.0",
        "np": "^6.5.0",
        "prettier": "^2.1.1",
        "pretty-quick": "^3.0.0",
        "react-styleguidist": "^11.0.8",
        "style-loader": "^1.2.1",
        "styled-components": "^5.1.1",
        "terser-webpack-plugin": "^4.1.0",
        "webpack": "^4.44.1",
        "webpack-cli": "^3.3.12",
        "webpack-dev-server": "^3.11.0"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not ie <= 11",
            "last 1 version",
            "Firefox ESR"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "husky": {
        "hooks": {
            "pre-commit": [
                "npm run lints"
            ]
        }
    },
    "lint-staged": {
        "src/**/*.{js,jsx,css,md,scss,ts,tsx}": [
            "eslint",
            "prettier --write --ignore-unknown",
            "git add"
        ]
    },
    "bugs": {
        "url": "https://github.com/muhimron90/muhi-ui/issues"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/muhimron90/muhi-ui.git"
    },
    "homepage": "https://muhimron90.github.io/muhi-ui",
    "publishConfig": {
        "access": "public"
    }
}
