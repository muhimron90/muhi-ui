{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": false,
                "useBuiltIns": "usage",
                "corejs": { "version": 3, "proposals": true }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "babel-plugin-styled-components",
            {
                "ssr": false,
                "displayName": false,
                "minify": false,
                "transpileTemplateLiterals": false
            }
        ],
        ["@babel/plugin-proposal-class-properties", { "loose": true }],
        "@babel/plugin-syntax-jsx"
    ]
}
